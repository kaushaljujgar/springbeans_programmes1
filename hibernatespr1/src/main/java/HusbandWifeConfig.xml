<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean class="com.jspiders.hibernatespr1.beans.Husband" name="husband1">

<property name="id" value="1"/>
<property name="name" value="Jethalal"/>
<property name="age" value="40"/>
<property name="wife" ref="wife1"/>
</bean>

<bean class="com.jspiders.hibernatespr1.beans.Wife" name="wife1">

<property name="id" value="1"/>
<property name="name" value="Daya"/>
<property name="age" value="35"/>

</bean>




</beans>