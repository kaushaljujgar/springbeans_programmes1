<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

<bean class="com.jspiders.hibernatespr1.beans.StudentBean" >

<property name="id"><value>1</value></property>
<property name="name"><value>kaushal</value></property>
<property name="email"><value>kaushal@gmail.com</value></property>

</bean>

<bean class="com.jspiders.hibernatespr1.beans.StudentBean" name="student1">

<property name="id" value="2"/>
<property name="name" value="Dinesh"/>
<property name="email" value="dinesh@gmail.com"/>

</bean>


<bean class="com.jspiders.hibernatespr1.beans.StudentBean" name="student2">

<constructor-arg name="id" value="3"/>
<constructor-arg name="name" value="pari"/>
<constructor-arg name="email" value="pari@gmail.com"/>
</bean>






</beans>